{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "client/build",
            "use": "@vercel/static"
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "client/build/index.html"
        }
    ],
    "buildCommand":"npm i --prefix client && npm run build --prefix client && npm i"
}
